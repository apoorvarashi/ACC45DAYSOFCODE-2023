#include<stdio.h>
#include<ctype.h>
int main()
{
    char ch;
    printf("Enter any character");
    scanf("%c",&ch);
    if(isalpha(ch))
    {
        if(islower(ch))
        printf("Upper form is %c\n",toupper(ch));
        else
        printf("It is not a lower case alphabet\n");
    }
    else
    printf("It is not an alphabet\n");
    return(0);
}
